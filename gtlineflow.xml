  <action type="control_user_interface">
    <useDefaultName>false</useDefaultName>
    <name>Control UI: gch = "com.linecorp.lineman.driver:id/button_choice_"; gtmodify = random(10, 30); secx = gtmodify; secy = random(15,35); xpix = random(200,700); ycal = random(-15,15); choice1x = 2090; choice2x = 2360; choice3x = 2635; choice1x = choice1x + ycal + 0; choice2x = choice2x + ycal + 0; choice3x = choicex</name>
    <script>gch = "com.linecorp.lineman.driver:id/button_choice_";
gtmodify = random(10, 30);
secx = gtmodify;
secy = random(15,35);
xpix = random(200,600);
ycal = random(-15,15);

choice1x = 1640;
choice2x = 1845;
choice3x = 2055;



choice1x = choice1x + ycal + 0;
choice2x = choice2x + ycal + 0;
choice3x = choice3x + ycal + 0;
choice1 = getTextById(gch+"1");
choice2 = getTextById(gch+"2");
choice3 = getTextById(gch+"3");
sleep(random(20,50));
if (choice1 == choice2)​
{
touchGesture(1, secy, newList(xpix, choice3x))​;
}​
else if (choice1 == choice3)​
{
touchGesture(1, secy, newList(xpix, choice2x))​;
}​
else if (choice2 == choice3)​
{
touchGesture(1, secy, newList(xpix, choice1x))​;
}​</script>
  </action>
